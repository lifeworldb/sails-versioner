{
  "name": "sails-versioner",
  "version": "0.1.1",
  "description": "sails versioner",
  "main": "index.js",
  "scripts": {
    "test": "echo 'No further automated tests have been implemented for this generator yet.'",
    "pretest": "npm run lint",
    "lint": "node ./node_modules/eslint/bin/eslint . --max-warnings=0"
  },
  "keywords": [
    "cts",
    "generator",
    "sails",
    "generate",
    "plugin",
    "controller",
    "version",
    "sails-versioner"
  ],
  "author": "lifeworldb",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.2.4",
    "dotenv": "^6.1.0",
    "fs-extra": "^7.0.1",
    "lodash": "^3.10.1",
    "merge-defaults": "^0.2.1",
    "pluralize": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "3.19.0"
  },
  "repository": "git@github.com:lifeworldb/sails-versioner.git",
  "sailsGenerator": {
    "type": "cts",
    "behavior": "Implements or overrides `sails generate sv`",
    "sailsVersion": "^1.0.0"
  },
  "private": false
}
